{
  "name": "reactivestates",
  "version": "2.0.1",
  "main": "dist/index.js",
  "module": "./dist/es2015/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.2",
    "@types/mocha": "2.2.48",
    "chai": "4.1.2",
    "del-cli": "^1.0.0",
    "mocha": "5.0.0",
    "source-map-support": "^0.5.9",
    "ts-node": "10.9.1",
    "tslint": "^5.11.0",
    "typescript": "5.0.4"
  },
  "peerDependencies": {
    "rxjs": "^7.8.0"
  },
  "scripts": {
    "clean": "del-cli dist",
    "tslint": "tslint --project tsconfig.json --config tslint.json",
    "compile": "tsc -p tsconfig.json",
    "compile-dist": "tsc -p tsconfig.dist.json && tsc -p tsconfig.dist.es2015.json",
    "dist": "npm run clean && npm run tslint && npm run test && npm run compile-dist",
    "test": "mocha --opts spec/mocha.opts src/**/*.test.ts",
    "test-watch": "mocha --opts spec/mocha.opts -w --watch-extensions ts src/**/*.test.ts"
  }
}
